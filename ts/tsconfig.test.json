{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "module": "CommonJS",
      "moduleResolution": "Node",
      "noEmit": true, 
      "allowJs": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src", "__tests__"]
  }
  